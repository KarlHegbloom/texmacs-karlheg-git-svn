    <apply|branch|env-math|env-math-dtd.it.tm>

    <apply|branch|env-theorem|env-theorem-dtd.it.tm>

    <apply|branch|env-float|env-float-dtd.it.tm>

    <apply|branch|env-default|env-default-dtd.it.tm>
