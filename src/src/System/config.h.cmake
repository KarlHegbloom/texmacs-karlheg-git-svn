/* src/System/config.h.cmake */

/* Enable experimental Cocoa port */
#cmakedefine AQUATEXMACS

/* Defined if ...-style argument passing works */
#cmakedefine DOTS_OK

/* Dynamic linking works */
#cmakedefine DYNAMIC_LINKING

/* Enable experimental style rewriting code */
#cmakedefine EXPERIMENTAL

/* Define to 1 if you have the `dlopen' function. */
#cmakedefine HAVE_DLOPEN

/* Define to 1 if the system has the type `FILE'. */
#cmakedefine HAVE_FILE

/* Define to 1 if you have the `gettimeofday' function. */
#cmakedefine HAVE_GETTIMEOFDAY

/* Define to 1 if the system has the type `intptr_t'. */
#cmakedefine HAVE_INTPTR_T

/* Define to 1 if you have the <inttypes.h> header file. */
#cmakedefine HAVE_INTTYPES_H

/* Define to 1 if you have the `dl' library (-ldl). */
#cmakedefine HAVE_LIBDL

/* Define to 1 if you have the `dld' library (-ldld). */
#cmakedefine HAVE_LIBDLD

/* Define to 1 if you have the <memory.h> header file. */
#cmakedefine HAVE_MEMORY_H

/* Define to 1 if you have the <pty.h> header file. */
#cmakedefine HAVE_PTY_H

/* Define to 1 if you have the `shl_load' function. */
#cmakedefine HAVE_SHL_LOAD

/* Define to 1 if you have the <stdint.h> header file. */
#cmakedefine HAVE_STDINT_H

/* Define to 1 if you have the <stdlib.h> header file. */
#cmakedefine HAVE_STDLIB_H

/* Define to 1 if you have the <strings.h> header file. */
#cmakedefine HAVE_STRINGS_H

/* Define to 1 if you have the <string.h> header file. */
#cmakedefine HAVE_STRING_H

/* Define to 1 if you have the <sys/stat.h> header file. */
#cmakedefine HAVE_SYS_STAT_H

/* Define to 1 if you have the <sys/types.h> header file. */
#cmakedefine HAVE_SYS_TYPES_H

/* Define to 1 if the system has the type `time_t'. */
#cmakedefine HAVE_TIME_T

/* Define to 1 if you have the <unistd.h> header file. */
#cmakedefine HAVE_UNISTD_H

/* Define to 1 if you have the <util.h> header file. */
#cmakedefine HAVE_UTIL_H

/* Link axel library with TeXmacs */
#cmakedefine LINKED_AXEL

/* Link cairo library with TeXmacs */
#cmakedefine LINKED_CAIRO

/* Link freetype library with TeXmacs */
#cmakedefine LINKED_FREETYPE

/* Link imlib2 library with TeXmacs */
#cmakedefine LINKED_IMLIB2

/* Disable fast memory allocator */
#cmakedefine NO_FAST_ALLOC

/* Define to the address where bug reports for this package should be sent. */
#cmakedefine PACKAGE_BUGREPORT

/* Define to the full name of this package. */
#cmakedefine PACKAGE_NAME

/* Define to the full name and version of this package. */
#cmakedefine PACKAGE_STRING

/* Define to the one symbol short name of this package. */
#cmakedefine PACKAGE_TARNAME

/* Define to the version of this package. */
#cmakedefine PACKAGE_VERSION

/* Enable experimental Qt port */
#cmakedefine QTTEXMACS

/* Define to 1 if you have the ANSI C header files. */
#cmakedefine STDC_HEADERS

/* Use axel library */
#cmakedefine USE_AXEL

/* Use cairo library */
#cmakedefine USE_CAIRO

/* Use freetype library */
#cmakedefine USE_FREETYPE

/* Use iconv library */
#cmakedefine USE_ICONV

/* Use imlib2 library */
#cmakedefine USE_IMLIB2

/* Define to 1 if the X Window System is missing or not being used. */
#cmakedefine X_DISPLAY_MISSING

/* Guile string size type */
#cmakedefine guile_str_size_t
