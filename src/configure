#--------------------------------------------------------------------
# Enable debug and optimizations
#--------------------------------------------------------------------

optimize_default="yes"

case "$GXX_VERSION" in
    3.3.3 | 3.3.4)
    ;;
    2.96 | 3.0 | 3.0.* | 3.1 | 3.1.* | 3.2 | 3.2.* | 3.3 | 3.3.*)
	case "${host}" in
	  i*86-*-linux-gnu* | i*86-*-freebsd*)
	    echo "configure: warning: using g++ 3.*, optimize without inline by default" 1>&2
	    optimize_default="no-inline"
	  ;;
	  *)
	    echo "configure: warning: using g++ 3.*, optimize without inline by default" 1>&2
	    optimize_default="no-inline"
	  ;;
	esac
    ;;
esac

# Check whether --enable-debug or --disable-debug was given.
if test "${enable_debug+set}" = set; then
  enableval="$enable_debug"
  :
else
  enable_debug="no"
fi


if test "$enable_debug" = "yes"; then
    if test "$GXX" = "yes"; then
        enable_debug="-ggdb"
    else
        enable_debug="-g3"
    fi
fi
if test "$enable_debug" = "no"; then
    echo "$ac_t""disabling debugging" 1>&6
    CONFIG_STRIP="strip"
    CONFIG_CXXDEBUG=""
else
    echo "$ac_t""enabling debugging, $enable_debug" 1>&6
    CONFIG_STRIP="true"
    CONFIG_CXXDEBUG="$enable_debug"
    optimize_default="no"
fi

# Check whether --enable-optimize or --disable-optimize was given.
if test "${enable_optimize+set}" = set; then
  enableval="$enable_optimize"
  :
else
  enable_optimize="$optimize_default"
fi


case "$enable_optimize" in
    yes)
	# keep optimization options
  	echo "$ac_t""enabling optimizations, $CONFIG_CXXOPTIMIZE" 1>&6 ;;
    no-inline)
	optimize_no_inline="-fno-default-inline -fno-inline"
	CONFIG_CXXOPTIMIZE="$CONFIG_CXXOPTIMIZE $optimize_no_inline"
  	echo "$ac_t""enabling optimizations, $CONFIG_CXXOPTIMIZE" 1>&6 ;;
    no)
	CONFIG_CXXOPTIMIZE=""
	echo "$ac_t""disabling optimizations" 1>&6 ;;
    *)
	CONFIG_CXXOPTIMIZE="$enable_optimize"
	echo "$ac_t""customizing optimizations, $enable_optimize" 1>&6 ;;
esac

